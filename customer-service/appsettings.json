{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    },
    "Urls": "https://*:5002"
  },
  "Consul": {
    "ConsulUrl": "http://localhost:8500",
    "ServiceName": "CustomerService",
    "ServiceUrl": "https://localhost:5002",
    "Tags": [ "api", "saga", "service" ],
    "TTLSeconds": 10,
    "TimeoutSeconds": 2
  }
}
